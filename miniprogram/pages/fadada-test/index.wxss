.wrap { padding: 16rpx; }
.title { font-size: 36rpx; font-weight: 600; margin-bottom: 24rpx; }
.subtitle { margin: 12rpx 0; color: #999; }
.split { height: 24rpx; }
button { margin: 8rpx 0; }
.ipt {
  border: 1px solid #e5e5e5;
  border-radius: 8rpx;
  padding: 12rpx;
  margin: 8rpx 0;
}
.log {
  height: 420rpx;
  border: 1px dashed #ddd;
  padding: 12rpx;
  border-radius: 8rpx;
  background: #0b0b0b;
  color: #dbdbdb;
  font-family: Menlo, Consolas, monospace;
  white-space: pre-wrap;
}
